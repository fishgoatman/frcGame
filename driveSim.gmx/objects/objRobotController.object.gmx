            <string>///driveSim function
if(controlling){
input = scrGetInput(id, controlScheme); //output of script: 0 thrust, 1 is steering wheel
var dir = robotId.dir;
output = scrDriveSim(input[0], input[1], dEnc, dir); //output of script: 0 is xPos, 1 is yPos, 2 is dir //maybe use pid
var dHPos = output[0];
var dVPos = output[1];
var dXPos = dHPos * dcos(dir - 90) + dVPos * dcos(dir);
var dYPos = dHPos * dsin(dir - 90) + dVPos * dsin(dir);
var dDir = output[2];
robotId.dXPos = dXPos;
robotId.dYPos = dYPos;
robotId.dDir = dDir;
var pEnc = enc;
enc = scrGetEncoderCounts();
dEnc = enc - pEnc;
}

</string>
